{
  "name": "livingdocs-magazine-boilerplate",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/livingdocsIO/livingdocs-magazine-boilerplate"
  },
  "scripts": {
    "design:dev": "NODE_ENV=development npm run start:magazine",
    "design:dev:mocked": "NODE_ENV=development npm run start:magazine:watch",
    "design:build": "NODE_ENV=production webpack --display-error-details",
    "design:publish": "npm run design:build && ldm design:publish design/dist",
    "now-build": "npm run design:build",
    "now-deploy": "now",
    "now-start": "ENVIRONMENT=now node app/index.js",
    "start:json-server": "nodemon --watch ./mocks --exec 'json-server ./mocks/db.js --routes ./mocks/routes.json --port 3001'",
    "start:magazine": "node app/index.js",
    "start:magazine:watch": "nodemon --watch conf --watch lib --watch app app/index.js",
    "start:magazine:mocked": "run-p start:json-server start:magazine",
    "start": "npm-run-all design:build design:dev:mocked"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.11.6",
    "chokidar": "^2.0.3",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.7",
    "html-loader": "^0.4.5",
    "html-minifier": "^0.5.6",
    "json-server": "^0.12.0",
    "livingdocs-manager": "^4.1.0",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.3",
    "now": "^11.4.1",
    "npm-run-all": "^4.1.2",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^2.0.6",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^3.0.3",
    "webpack-dev-middleware": "^3.1.0",
    "webpack-hot-middleware": "^2.21.2"
  },
  "dependencies": {
    "@livingdocs/conf": "^1.0.1",
    "@livingdocs/node-sdk": "^0.1.0",
    "combined-slugify": "^2.1.0",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "moment": "^2.11.0",
    "photoswipe": "^4.1.2"
  }
}
