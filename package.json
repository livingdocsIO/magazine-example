{
  "name": "livingdocs-magazine-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "design:dev": "NODE_ENV=development npm run start:magazine",
    "design:dev:mocked": "NODE_ENV=development run-p start:json-server start:magazine:watch",
    "design:build": "NODE_ENV=production webpack --display-error-details",
    "design:publish": "npm run design:build && ldm design:publish design/dist",
    "start:json-server": "nodemon --watch ./mocks --exec 'json-server ./mocks/db.js --routes ./mocks/routes.json --port 3001'",
    "start:magazine": "node app/index.js",
    "start:magazine:watch": "nodemon --watch conf --watch lib --watch app app/index.js",
    "start:magazine:mocked": "run-p start:json-server start:magazine",
    "start": "npm run design:dev:mocked"
  },
  "devDependencies": {
    "chokidar": "^1.7.0",
    "clean-webpack-plugin": "^0.1.14",
    "color": "^0.6.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^1.1.5",
    "html-loader": "^0.4.3",
    "html-minifier": "^0.5.6",
    "json-server": "^0.12.0",
    "livingdocs-manager": "^4.1.0",
    "node-sass": "^3.4.2",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "openpack": "^1.0.0",
    "postcss-loader": "^1.1.1",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "susy": "2.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  },
  "dependencies": {
    "@livingdocs/conf": "^1.0.1",
    "combined-slugify": "^2.1.0",
    "compression": "^1.7.1",
    "express": "^4.1.1",
    "handlebars": "^4.0.11",
    "photoswipe": "^4.1.2"
  }
}
